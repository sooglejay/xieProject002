CREATE TABLE `ziyan`.`selling_area` ( `id` INT NOT NULL COMMENT '索引' , `city` VARCHAR NOT NULL COMMENT '地市' , `county` VARCHAR NOT NULL COMMENT '区县' , `code` VARCHAR NOT NULL COMMENT '渠道编码' , `selling_area_name` VARCHAR NOT NULL COMMENT '营业厅' , `area_name` VARCHAR NOT NULL COMMENT '城东城西等名称' , `grid_name` VARCHAR NOT NULL COMMENT '网格名称' , `account_name` VARCHAR NOT NULL COMMENT '账号' , PRIMARY KEY (`id`)) ENGINE = InnoDB;